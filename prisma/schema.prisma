generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Carro {
  id              Int      @id @default(autoincrement())
  marca           String
  modelo          String
  ano             Int
  quilometragem   Int
  descricao       String
  url_imagem      String
  caracteristicas String   @map("caracter√≠sticas") // Mapeando para o nome da coluna no BD
}